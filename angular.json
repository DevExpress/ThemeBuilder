{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "devextreme-themebuilder-app": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "prefix": "app",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@angular-builders/custom-webpack:browser",
                    "options": {
                        "customWebpackConfig": { "path": "./webpack.config.js" },
                        "outputPath": "dist/devextreme-themebuilder-app",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "allowedCommonJsDependencies": [
                            "devexpress-diagram",
                            "devexpress-gantt",
                            "devextreme-quill",
                            "file-saver",
                            "jszip",
                            "jszip-utils",
                            "devextreme-themebuilder"
                        ],
                        "assets": [
                            "src/favicon.ico",
                            "src/assets",
                            "src/images",
                            {
                                "glob": "dx.material.blue.light.compact.css",
                                "input": "./node_modules/devextreme/dist/css",
                                "output": "content/css"
                            },
                            {
                                "glob": "**/*",
                                "input": "./node_modules/devextreme/dist/css/icons",
                                "output": "content/css/icons"
                            },
                            {
                                "glob": "**/*",
                                "input": "./node_modules/devextreme/dist/css/fonts",
                                "output": "content/css/fonts"
                            }
                        ],
                        "styles": [
                            "src/styles.css"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-builders/custom-webpack:dev-server",
                    "options": {
                        "buildTarget": "devextreme-themebuilder-app:build"
                    },
                    "configurations": {
                        "production": {
                            "buildTarget": "devextreme-themebuilder-app:build:production"
                        },
                        "development": {
                            "buildTarget": "devextreme-themebuilder-app:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "buildTarget": "devextreme-themebuilder-app:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
      "analytics": false
    }
}
